{
    "name": "gnucobol",
    "displayName": "GnuCOBOL",
    "description": "GnuCOBOL Syntax extension for COBOL",
    "author": {
        "name": "Stephen Gennard",
        "email": "stephen@gennard.net"
    },
    "license": "MIT",
    "keywords": [
        "GnuCOBOL"
    ],
    "version": "0.7.8",
    "publisher": "bitlang",
    "engines": {
        "vscode": "^1.60.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spgennard/vscode_cobol4gnucobol"
    },
    "homepage": "https://spgennard.github.io/vscode_cobol4gnucobol/",
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onFileSystem:file",
        "onLanguage:GnuCOBOL",
        "onLanguage:gnucobol",
        "workspaceContains:**/*.cobol",
        "workspaceContains:**/*.cob",
        "workspaceContains:**/*.cbl",
        "workspaceContains:**/*.cpy"
    ],
    "main": "./out/extension.js",
    "icon": "gnucobol.png",
    "contributes": {
        "configuration": {},
        "languages": [
            {
                "id": "GnuCOBOL",
                "aliases": [
                    "GnuCOBOL",
                    "gnucobol"
                ],
                "extensions": [
                    ".cobol",
                    ".cbl",
                    ".cpy",
                    ".copybook",
                    ".cobcopy",
                    ".cob"
                ],
                "configuration": "./gnucobol.configuration.json"
            },
            {
                "id": "COBOL_GNU_LISTFILE",
                "extensions": [],
                "configuration": "./gnucobol.configuration.json"
            },
            {
                "id": "COBOL_GNU_DUMPFILE",
                "extensions": [
                    ".dump"
                ],
                "configuration": "./gnucobol.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "GnuCOBOL",
                "path": "./syntaxes/GnuCOBOL.tmLanguage.json",
                "scopeName": "source.gnucobol"
            },
            {
                "language": "COBOL_GNU_LISTFILE",
                "scopeName": "source.gnucobol_listfile",
                "path": "./syntaxes/COBOL_gnu_listfile.tmLanguage.json"
            },
            {
                "language": "COBOL_GNU_DUMPFILE",
                "scopeName": "source.gnucobol_dumpfile",
                "path": "./syntaxes/COBOL_gnu_dumpfile.tmLanguage.json"
            }
        ],
        "commands": [],
        "configurationDefaults": {
            "[GnuCOBOL]": {
                "editor.semanticHighlighting.enabled": false,
                "editor.insertSpaces": true,
                "files.autoGuessEncoding": false,
                "editor.autoIndent": "full",
                "editor.formatOnType": false,
                "editor.rulers": [
                    6,
                    7,
                    72
                ],
                "editor.detectIndentation": false,
                "editor.wordSeparators": "`~!#$%^&*()=+[{]}\\|;:'\",.<>/?"
            }
        },
        "menus": {},
        "keybindings": [],
        "problemPatterns": [
            {
                "name": "gnucobol-cobc",
                "regexp": "^(.*): ?(\\d+): ([eE]rror|[wW]arning): ([^[]*)(\\[(.*)\\])?$",
                "file": 1,
                "line": 2,
                "severity": 3,
                "message": 4,
                "code": 6
            },
            {
                "name": "gnucobol3-cobc",
                "regexp": "^(.*): ?(\\d+): (error|warning): ([^[]*)(\\[(.*)\\])?$",
                "file": 1,
                "line": 2,
                "severity": 3,
                "message": 4,
                "code": 6
            },
            {
                "name": "gnucobol-warning-cobc",
                "regexp": "^(.*): ?(\\d+): ([wW]arning|[wW]arnung|[wW]aarschuwing|[aA]lerta|avertissement|упозорење): ([^[]*)(\\[(.*)\\])?$",
                "file": 1,
                "line": 2,
                "message": 4,
                "code": 6
            },
            {
                "name": "gnucobol-error-cobc",
                "regexp": "^(.*): ?(\\d+): *([eE]rror|[fF]ehler|[fF]out||[eE]rrores|[eE]rrores|erreur|грешка): ([^[]*)(\\[(.*)\\])?$",
                "file": 1,
                "line": 2,
                "message": 4,
                "code": 6
            },
            {
                "name": "gnucobol3-note-cobc",
                "regexp": "^(.*): ?(\\d+): ([nN]ote|Anmerkung|[nN]ota): ([^[]*)(\\[(.*)\\])?$",
                "file": 1,
                "line": 2,
                "message": 4,
                "code": 6
            }
        ],
        "problemMatchers": [
            {
                "name": "gnucobol-cobc",
                "owner": "cobol",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": "$gnucobol-cobc",
                "source": "GnuCOBOL1+2"
            },
            {
                "name": "gnucobol3-cobc",
                "owner": "cobol",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": "$gnucobol3-cobc",
                "source": "GnuCOBOL3"
            },
            {
                "name": "gnucobol3-warning-cobc",
                "owner": "cobol",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": "$gnucobol-warning-cobc",
                "severity": "warning",
                "source": "GnuCOBOL3"
            },
            {
                "name": "gnucobol3-error-cobc",
                "owner": "cobol",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": "$gnucobol-error-cobc",
                "severity": "error",
                "source": "GnuCOBOL3"
            },
            {
                "name": "gnucobol3-note-cobc",
                "owner": "cobol",
                "fileLocation": [
                    "absolute"
                ],
                "pattern": "$gnucobol3-note-cobc",
                "severity": "info",
                "source": "GnuCOBOL3"
            }
        ]
    },
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile && webpack --mode production",
        "webpack": "npm run compile && webpack --mode development",
        "compile": "tsc -p ./",
        "test-compile": "tsc -p ./",
        "test": "node ./out/test/runTest.js",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./",
	"find-deadcode": "./node_modules/.bin/ts-prune | grep -v \"used in module\""
    },
    "dependencies": {
        "ts-prune": "^0.10.1",
        "typescript-string-operations": "^1.4.1"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.9.1",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.2",
        "vsce": "^1.99.0",
        "vscode-test": "^1.6.1"
    }
}
